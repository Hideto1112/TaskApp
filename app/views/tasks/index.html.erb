<% provide(:title, 'All Tasks') %>
<h1>タスク一覧</h1>

<div class="<row>">
  <div class="col-md-10 col-md-offset-1">
    <table class="table table-condensed table-hover" id="table-tasks">
      <thead>
        <tr>
          <th>名称</th>
          <th>作成日</th>
        </tr>
      </thead>
  
      <tbody>
        <tr>
          <td><%= link_to @tasks.find(1).name %></td>
        </tr>
        
      </tbody>
    </table>
  </div>
</div>